{
  "name": "glslint",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "source": "glsl/src/index.js",
  "module": "html/main.js",
  "scripts": {
    "test": "echo \"Error: no test specified\" && exit 1",
    "wasm": "/Applications/cheerp/bin/clang++ -std=c++14 -target cheerp -cheerp-mode=wasm -cheerp-linear-heap-size=16 -Wall -cheerp-pretty-code -cheerp-wasm-loader=html/dataflow.js -cheerp-wasm-file=dataflow.wasm -O3 -o html/dataflow.wasm ./dataflow/src/*.cpp ./dataflow/src/model/*.cpp ./dataflow/src/model/ops/*.cpp",
    "wasm-prod": "/Applications/cheerp/bin/clang++ -std=c++14 -target cheerp -cheerp-mode=wasm -cheerp-linear-heap-size=16 -Wall -cheerp-wasm-loader=html/dataflow.js -cheerp-wasm-file=dataflow.wasm -O0 -o html/dataflow.wasm ./dataflow/src/*.cpp ./dataflow/src/model/*.cpp ./dataflow/src/model/ops/*.cpp",
    "server": "http-server -p 3355 -c0 ./html",
    "dataflow-tests": "cd dataflow && scons && ./dataflow_tests && scons -c",
    "dataflow-tests-valgrind": "cd dataflow && scons && valgrind ./dataflow_tests && scons -c",
    "js": "./node_modules/.bin/microbundle -o html/main.js --compress=false -f cjs",
    "js-watch": "./node_modules/.bin/microbundle watch -o html/main.js --compress=false -f cjs",
    "update-enum": "node tools/gen_keywords_enums.js"
  },
  "author": "",
  "license": "ISC",
  "dependencies": {
    "glsl-parser": "^2.0.0",
    "glsl-tokenizer": "^2.1.2",
    "microbundle": "^0.4.4",
    "quill": "^1.3.6"
  },
  "devDependencies": {}
}
